---
interface Props {
	title: string;
	description?: string;
	image?: string
}

const { title,description,image } = Astro.props;

import Header from "../components/Header.astro";

import Pattern from "../components/Pattern.astro";

import Footer from "../components/Footer.astro";


import '@fontsource/shippori-mincho';


---

<!doctype html>
<html lang="ja">
	<head>

		<meta name="astro-view-transitions-fallback" content="animate">
	
		<meta charset="utf-8" />
		<meta http-equiv="X-UA-Compatible" content="IE=edge">
		<meta name="viewport" content="width=device-width,initial-scale=1.0" />
		<meta http-equiv="X-UA-Compatible" content="IE=edge" />
		
		<title>セミオーダー</title>
		<meta name="description" content="セミオーダーについて" />
		<meta name="format-detection" content="telephone=no" />
        <meta name="googlebot" content="noindex">
        <meta name="robots" content="noindex,nofollow" />

		<link rel="icon" href="/king.png">
		<link rel="apple-touch-icon" href="/king.png" sizes="180x180">

		<meta property="og:url" content="https://pircamabui.com/semi_order/">
		<meta property="og:type" content="website">
		<meta property="og:title" content="セミオーダー">
		<meta property="og:description" content="セミオーダーについて">
		<meta property="og:image" content="/samale-min.webp">
	

		<title>{title}</title>
	</head>

	<body>

        <Header />
        <Pattern />
        
		<Footer />
		<slot />
	</body>
</html>
