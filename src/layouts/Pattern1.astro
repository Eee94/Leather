---
interface Props {
	title: string;
	description?: string;
	image?: string
}

const { title,description,image } = Astro.props;

import Header from "../components/Header.astro";


import Pa1 from "../components/Pa1.astro";
import Back from "../components/Back.astro";
import Footer from "../components/Footer.astro";


import '@fontsource/shippori-mincho';

---

<!doctype html>
<html lang="ja">
	<head>

		<meta name="astro-view-transitions-fallback" content="animate">
	
		<meta charset="utf-8" />
		<meta http-equiv="X-UA-Compatible" content="IE=edge">
		<meta name="viewport" content="width=device-width,initial-scale=1.0" />
		<meta http-equiv="X-UA-Compatible" content="IE=edge" />
		
		<title>パターンオーダー キーケース</title>
		<meta name="description" content="パターンオーダー キーケース" />
		<meta name="format-detection" content="telephone=no" />
        <meta name="googlebot" content="noindex">
        <meta name="robots" content="noindex,nofollow" />

		<link rel="icon" href="/king.png">
		<link rel="apple-touch-icon" href="/king.png" sizes="180x180">
	

		<title>{title}</title>
	</head>

	<body>

        <Header />

        <Pa1 />		
		<Back />
		<Footer />
		<slot />
	</body>
</html>